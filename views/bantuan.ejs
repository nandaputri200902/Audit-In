            <div class="contact-email">
              <i class="ion-ios-email-outline"></i>
              <h3>Email</h3>
              <p>
                <a href="mailto:ahmadhasyimi674@gmail.com"
                  >ahmadhasyimi674@gmail.com</a
                >
              </p>
            </div>
          </div>
        </div>

        <div class="form">
          <div id="sendmessage">Pesan anda telah terkirim!</div>
          <div id="errormessage"></div>
          <form
            action="/kirimEmail"
            method="post"
            role="form"
            class="contactForm"
          >
            <div class="form-row">
              <div class="form-group col-md-6">
                <input
                  type="text"
                  name="name"
                  class="form-control"
                  id="name"
                  placeholder="Nama anda."
                  data-rule="minlen:4"
                  data-msg="Silakan masukan setidaknya 4 karakter."
                />
                <div class="validation"></div>
              </div>
              <div class="form-group col-md-6">
                <input
                  type="email"
                  class="form-control"
                  name="from"
                  id="email"
                  placeholder="Email anda."
                  data-rule="email"
                  data-msg="Silakan masukan email anda."
                />
                <div class="validation"></div>
              </div>
            </div>
            <div class="form-group">
              <input
                type="text"
                class="form-control"
                name="subject"
                id="subject"
                placeholder="Subjek"
                data-rule="minlen:4"
                data-msg="Silakan masukan setidaknya 8 karakter subjek."
              />
              <div class="validation"></div>
            </div>
            <div class="form-group">
              <textarea
                class="form-control"
                name="text"
                rows="5"
                data-rule="required"
                data-msg="Silakan ketikan isi hati anda <3"
                placeholder="Pesan anda."
              ></textarea>
              <div class="validation"></div>
            </div>
            <div class="text-center">
              <button type="submit">Kirim!</button>
            </div>
          </form>
        </div>
      </div>
